{
  "name": "marx.sh",
  "version": "1.0.0",
  "description": "A personal site, or something",
	"type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-ts": "tsc --build --watch",
    "compile-main-sass": "sass --no-source-map --style=\"compressed\" -w assets/scss/main.scss assets/css/main.css",
    "compile-minimal-sass": "sass --no-source-map --style=\"compressed\" -w assets/scss/minimal.scss assets/css/minimal.css",
    "minify": "uglifyjs --compress --mangle -o assets/js/main.min.js -- assets/js/main.js && uglifyjs --compress --mangle -o assets/js/buttons.min.js -- assets/js/buttons.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williamwmarx/marx.sh.git"
  },
