  "bugs": {
    "url": "https://github.com/williamwmarx/marx.sh/issues"
  },
  "homepage": "https://github.com/williamwmarx/marx.sh#readme",
  "dependencies": {
    "@openpgp/web-stream-tools": "^0.0.13",
    "openpgp": "^5.5.0",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "uglify-js": "^3.17.4"
  }
}
